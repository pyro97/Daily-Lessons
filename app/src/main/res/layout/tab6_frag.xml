<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/sabOrario"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="14dp"
                android:layout_marginTop="17dp"
                android:background="@color/colorPrimary"
                android:editable="false"
                android:text="13"
                android:textColor="@android:color/background_light" />

            <TextView
                android:id="@+id/sabMateria"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="32dp"
                android:editable="false"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/sabClasse"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/sabMateria"
                android:layout_marginEnd="40dp"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp" />

            <Button
                android:id="@+id/sab1Orario"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/sab1Materia"
                android:layout_alignStart="@+id/sabOrario"
                android:layout_marginStart="0dp"
                android:background="@android:color/holo_orange_dark"
                android:text="15"
                android:textColor="@android:color/background_light" />

            <TextView
                android:id="@+id/sab1Materia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="111dp"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/sab1Classe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/sab1Orario"
                android:layout_alignStart="@+id/sabClasse"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp" />

            <Button
                android:id="@+id/sab3Orario"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/sabOrario"
                android:layout_marginTop="246dp"
                android:background="@android:color/holo_red_dark"
                android:text="17"
                android:textColor="@android:color/background_light" />

            <TextView
                android:id="@+id/sab2Materia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/sab2Orario"
                android:layout_centerHorizontal="true"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/sab2Classe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/sab2Orario"
                android:layout_alignStart="@+id/sabClasse"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp" />

            <Button
                android:id="@+id/sab2Orario"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/sabOrario"
                android:layout_marginTop="169dp"
                android:background="@android:color/holo_blue_bright"
                android:text="16"
                android:textColor="@android:color/background_light" />

            <TextView
                android:id="@+id/sab3Classe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/sab3Orario"
                android:layout_alignStart="@+id/sabClasse"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/sab3Materia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/sab3Orario"
                android:layout_centerHorizontal="true"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/sab4Orario"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/sabOrario"
                android:layout_marginTop="319dp"
                android:background="@android:color/holo_orange_light"
                android:text="19"
                android:textColor="@android:color/background_light" />

            <TextView
                android:id="@+id/sab4Classe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/sab4Orario"
                android:layout_alignStart="@+id/sabClasse"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/sab4Materia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/sab4Orario"
                android:layout_centerHorizontal="true"
                android:text="TextView"
                android:textColor="@android:color/background_dark"
                android:textSize="16sp"
                android:textStyle="bold" />

        </RelativeLayout>
    </ScrollView>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabio5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="62dp"
        android:layout_marginEnd="157dp"
        android:clickable="true"
        android:tint="@android:color/background_light"
        app:backgroundTint="@android:color/holo_red_dark"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/fabio5"
        android:layout_marginEnd="91dp"
        android:clickable="true"
        app:backgroundTint="@android:color/holo_red_dark"
        app:srcCompat="@android:drawable/ic_menu_edit" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/fabio5"
        android:layout_marginEnd="24dp"
        android:clickable="true"
        android:tint="@android:color/background_light"
        app:backgroundTint="@android:color/holo_red_dark"
        app:srcCompat="@android:drawable/ic_menu_delete" />

</RelativeLayout>